{
  "name": "@sciice/component",
  "version": "1.0.2",
  "scripts": {
    "start": "cross-env NODE_ENV=development rollup -c scripts/dev.js --watch",
    "clean": "rimraf dist",
    "build": "yarn clean && cross-env NODE_ENV=production rollup -c scripts/prod.js",
    "lint-staged": "lint-staged",
    "test": "jest -u",
    "prettier": "node ./scripts/prettier.js"
  },
  "main": "dist/index.js",
  "types": "src/component/index.ts",
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-external-helpers": "^7.2.0",
    "@babel/plugin-proposal-class-properties": "^7.2.3",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/polyfill": "^7.2.5",
    "@babel/preset-env": "^7.2.3",
    "@babel/preset-react": "^7.0.0",
    "@types/react": "^16.7.17",
    "@types/react-dom": "^16.0.11",
    "antd": "^3.11.2",
    "axios": "^0.18.0",
    "babel-core": "^7.0.0-0",
    "babel-jest": "^23.6.0",
    "classnames": "^2.2.6",
    "cross-env": "^5.2.0",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.7.1",
    "glob": "^7.1.3",
    "husky": "^1.2.1",
    "jest": "^23.6.0",
    "less": "^3.9.0",
    "less-plugin-npm-import": "^2.1.0",
    "lint-staged": "^8.1.0",
    "prettier": "^1.15.3",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "rimraf": "^2.6.2",
    "rollup": "^0.68.1",
    "rollup-plugin-alias": "^1.5.1",
    "rollup-plugin-babel": "^4.1.0",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-filesize": "^5.0.1",
    "rollup-plugin-json": "^3.1.0",
    "rollup-plugin-livereload": "^0.6.0",
    "rollup-plugin-node-resolve": "^4.0.0",
    "rollup-plugin-postcss": "^1.6.3",
    "rollup-plugin-progress": "^0.4.0",
    "rollup-plugin-replace": "^2.1.0",
    "rollup-plugin-serve": "^0.6.0",
    "rollup-plugin-terser": "^4.0.2"
  },
  "lint-staged": {
    "**/*.{js,ts,tsx,json,jsx,less}": [
      "node ./scripts/prettier.js",
      "git add"
    ]
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint-staged"
    }
  },
  "jest": {
    "setupTestFrameworkScriptFile": "<rootDir>test/setupTests.js"
  }
}
